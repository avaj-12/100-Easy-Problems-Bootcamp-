#include <bits/stdc++.h>
#define fastio                        \
    ios_base::sync_with_stdio(false); \
    cin.tie(NULL);                    \
    cout.tie(NULL);
 
using namespace std;
int main()
{
    cin.tie(NULL);
    int n;
    cin >> n;
    int num[n];
    for (int i = 0; i < n; i++)
    {
        cin >> num[i];
    }
    for (int i = n - 1; i >= 0; i--)
    {
        cout << num[i] << " ";
    }
    return 0;
}
